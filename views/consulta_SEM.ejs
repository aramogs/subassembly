<%- include ("./static/header") %>
<%- include ("./static/sidebar") %>
<%- include ("./static/navbar") %>




<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="http://10.56.99.21:3017/mainMenu" class="text-info"><span class="fas fa-arrow-circle-left text-info"></span> Menu</a></li>
    <li class="breadcrumb-item active" aria-current="page">Consulta Ubicaciones SEM</li>
  </ol>
</nav>



<div class="col-lg-12 frame">

  <div class="row">
    <div class="col-lg-4"></div>

    <div class="col-lg-4">
      <div class="card">
        <div class="card-header d-none d-md-block"><strong>Datos de Usuario</strong></div>
        <div class="card-body">
          <table class="table table-sm borderless">
            <tbody>
              <tr>
                <td><small><strong>ID:</strong></small></td>
                <td><small id="user_id"><%= user_id %></small> </td>
              </tr>
              <tr>
                <td><small><strong>Nombre:</strong></small></td>
                <td><small id="user_name"><%= user_name  %></small> </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <div class="col-lg-4"></div>
  </div>

  <div class="container text-center mb-5">
    <div class="row">
      <div class="col-lg-3"></div>
      <div class="col-lg-6">


        <h4 class="text-center animate__animated animate__bounceIn d-none d-md-block" display: inline-block>Consulta
          Ubicaciones SEM:</h4>
        <span class="fas fa-search text-info mt-2" style="font-size: 2rem;"></span>
        <form id="submitSerial">
          <div class="form-group">
            <label for="serial_num">Storage_unit:(S) | Part_Number:(P) | Mandrel:(M)</label>
            <input type="text" class="form-control" name="serial_num" id="serial_num" aria-describedby="helpId"
              placeholder="" autocomplete="off">
            <small id="helpId" class="form-text text-muted"><i class="fas fa-barcode"
                style="font-size: 2rem;"></i></small>
          </div>
        </form>
        <div class="alert alert-danger animate__flipOutX animate__animated role=" alert" id="alerta_prefijo">Error:
          Verificar escaneo
        </div>
      </div>
      <div class="col-lg-3"></div>
    </div>

  </div>

  <div class="d-none d-lg-block">
    <br><br><br><br><br><br><br><br><br><br><br><br><br>

  </div>
</div>





<!-- Small modal Error-->
<div class="modal fade" id="modalError" tabindex="-1" role="dialog" aria-labelledby="modelTitleId" aria-hidden="true">
  <div class="modal-dialog modal-sm" role="document">
    <div class="modal-content">
      <div class="modal-header card-mov">
        <img src="/img/sap.png" alt="SAP" srcset="" class="card-mov">
        <h4 class="card-mov">Error:</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        </button>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-2">
            <i class="fas fa-exclamation-circle text-danger" style="font-size: 2rem;"> </i>
          </div>
          <div class="col">
            <p id="errorText"></p>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary btnCerrar" data-dismiss="modal">Cerrar</button>
      </div>
    </div>
  </div>
</div>


<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
  aria-hidden="true">
  <div class="modal-dialog modal-dialog-scrollable" role="document">
    <div class="modal-content">

      <div class="modal-body">


        <table id="tabla_consulta" class="table table-hover  text-center table-bordered table-sm table-striped">
          <thead class="thead-dark">
            <tr class="header">
              <th scope="col">Storage Bin</th>
              <th scope="col">Serial</th>
              <th scope="col">FIFO</th>
            </tr>
          </thead>
          <tbody>
            <!-- INFO AGREGADA DESDE JAVASCRIPT -->
          </tbody>
        </table>

      </div>

      <div class="modal-footer">
        <button type="button" class="btn btn-danger btnCerrar" data-dismiss="modal">Cerrar <span
            class="fas fa-times-circle"></span></button>
        <button type="button" onclick="location.href='/transferSEM'" class="btn btn-primary">Transferir <span
            class="fas fa-arrow-circle-right"></span></button>

      </div>


    </div>
  </div>
</div>


<!-- Small modal Success-->
<div class="modal fade" id="modalSuccess" tabindex="-1" role="dialog" aria-labelledby="modelTitleId" aria-hidden="true">
  <div class="modal-dialog modal-sm" role="document">
    <div class="modal-content">
      <div class="modal-header card-mov">
        <img src="/img/sap.png" alt="SAP" srcset="" class="card-mov">
        <h4 class="card-mov">Movimiento exitoso</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        </button>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-2">
            <i class="far fa-check-square text-success" style="font-size: 2rem;"> </i>
          </div>
          <div class="col">
            <p id="successText"></p>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary btnCerrar" data-dismiss="modal">Cerrar</button>
      </div>
    </div>
  </div>
</div>


<!-- Modal Spinners -->
<div class="modal fade" id="modalSpinner" tabindex="-1" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header text-center">
        <h2 class="modal-title "> <span class="text-secondary  fas fa-stopwatch"></span>
          Procesando</h2>
        <div class="spinner-grow spinner-grow-sm text-info mt-4 cdelay-1" role="status">
          <span class="sr-only">Loading...</span>
        </div>
        <div class="spinner-grow spinner-grow-sm text-primary mt-4 cdelay-2" role="status">
          <span class="sr-only">Loading...</span>
        </div>
        <div class="spinner-grow spinner-grow-sm text-secondary mt-4 cdelay-3" role="status">
          <span class="sr-only">Loading...</span>
        </div>
      </div>
    </div>
  </div>
</div>

<%- include ("./static/footer") %>
<script type="text/javascript" src="/js/functions/consulta_SEM.js"></script>

</body>

</html>